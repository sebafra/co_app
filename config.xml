<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns 		= "http://www.w3.org/ns/widgets"
        	xmlns:gap 	= "http://phonegap.com/ns/1.0"
        	id        	= "com.diproach.consorcio.movil"
        	versionCode	= "9"
        	version   	= "0.00.09">

    <!-- versionCode is optional and Android only -->

    <name>Consorcio Movil</name>

    <description>
        Consorcios
    </description>

    <author href="http://diproach.com" email="info@diproach.com">
        Diproach
    </author>

	<gap:splash src="splash.png" width="720" height="1280" />

	<preference name="orientation" value="portrait" />
<!--     <preference name="fullscreen" value="true" /> -->
    <preference name="load-url-timeout" value="60000" />
	<preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />

    
    <feature name="http://api.phonegap.com/1.0/file"/>
	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	<feature name="http://api.phonegap.com/1.0/media"/>
	<feature name="http://api.phonegap.com/1.0/network"/>
	<feature name="http://api.phonegap.com/1.0/notification"/>

	
	<plugin name="DatePickerPlugin" value="com.phonegap.plugins.DatePickerPlugin"/>
	<plugin name="SoftKeyBoard" value="com.phonegap.plugins.SoftKeyboard" />
	
	
	<access origin="*" />


	<icon src="icon.png" />

</widget>